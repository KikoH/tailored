<section id="contact" class="section-padding">
    <div class="container">
        <div class="row">
            <div class="col-sm-8 col-md-offset-2">
                <div class="text-center">
                    <h2 class="center-title">Contact Us</h2>
                    <span class="center-border-line"></span>
                    <p class="sub-text"></p>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-7">
                <div class="contact-form wow animated fadeInLeft" data-wow-delay=".2s">
                    <%= form_tag contacts_path, remote: :true do %>
                    <form role="form" id="feedbackForm">
                        <div class="row">
                            <div class="col-md-9">
                                <div class="form-group">
                                    <label class="control-label" for="name">Name *</label>
                                    <div>
                                        <%= text_field_tag :name, nil, class: "form-control", placeholder: "Enter your name" %>
                                    </div>
                                    <span class="help-block" style="display: none;">Please enter your name.</span>
                                </div> 
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-9">
                                <div class="form-group">
                                    <label class="control-label" for="email">Email Address *</label>
                                    <div>
                                        <%= text_field_tag :email, nil, class: "form-control", placeholder: "Enter your email" %>
                                    </div>
                                    <span class="help-block" style="display: none;">Please enter a valid e-mail address.</span>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-9">
                                <div class="form-group">
                                    <label class="control-label" for="email">How did you hear about us?</label>
                                    <div>
                                        <%= text_field_tag :hear, nil, class: "form-control" %>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-9">
                                <div class="form-group">
                                    <label class="control-label" for="email">Can we publish your wedding images on our social media?</label>
                                    <div>
                                        <%= radio_button_tag :can_publish,  "yes" %> Yes
                                        <%= radio_button_tag :can_publish, 'no' %> No
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-9">
                                <div class="form-group">
                                    <label class="control-label" for="email">Where will your wedding be held? (if decided)</label>
                                    <div>
                                        <%= text_field_tag :held, nil, class: "form-control" %>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label" for="message">Message *</label>
                            <div>
                                <%= text_area_tag :message, nil, class: "form-control", placeholder: "Enter your message", rows: "5", cols: "30" %>
                            </div>
                            <span class="help-block" style="display: none;">Please enter a message.</span>
                        </div>
                        <%= submit_tag(value='Send Feedback', class: "btn btn-dark-bg btn-lg") %>
                    </form>
                    <div class="errors"></div>
                    <% end %>
                </div><!--contact form-->
            </div><!--col-7-->
            <div class="col-sm-5">
                <h4>Leave a message</h4>
                <p>
                Do you have a question? Want to book with us? Just want to say hi? Please fill out the form.<br>
                    Or contact us directly: <br>
                    Phone: +201272377238, +201207329333
                </p>
                <br>
                <br>
                <div id="map" style="height: 250px;"></div>
            </div>
        </div><!--row-->
    </div><!--container-->
</section><!--contact section end-->

